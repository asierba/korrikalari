{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "FileSystemCache.fs",
        "Coordinate.fs",
        "StravaClient.fs",
        "GoogleMapsClient.fs",
        "OsmConverter.fs",
        "StreetRepository.fs",
        "WebClient.fs",
        "MainMiddleware.fs",
        "Startup.fs",
        "Program.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc": "1.0.0-preview2-*"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.AspNetCore" : "1.1.0-preview1-final",
        "System.Net.Http": "4.3.0",
        "Newtonsoft.Json" :"9.0.1",
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        },
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160629"
